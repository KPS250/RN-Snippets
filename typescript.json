  "Redux Toolkit-Slice": {
    "prefix": "sl",
    "body": [
      "import { createSlice, PayloadAction } from '@reduxjs/toolkit';",
      "",
      "type [Custom]State = {",
      "  mobileNumber: string;",
      "};",
      "",
      "const initialState: [Custom]State = {",
      "  mobileNumber: '',",
      "};",
      "",
      "export const $0 = createSlice({",
      "  name: 'user',",
      "",
      "  initialState,",
      "  reducers: {",
      "    setUserMobile: (state, action: PayloadAction<any>) => {",
      "      state.mobileNumber = action.payload;",
      "    },",
      "    resetUserMobile: (state) => {",
      "      state.mobileNumber = '';",
      "    },",
      "  },",
      "});",
      "",
      "export const { setUserMobile, resetUserMobile } = $0.actions;",
      "",
      "export default $0.reducer;",
      ""
    ],
    "description": "Redux Toolkit-Slice"
  }
}
