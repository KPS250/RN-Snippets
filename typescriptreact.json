      "  const Strings = useAppSelector((state: any) => state.app.translations);",
      "",
      "  return (",
      "    <View style={[styles.container, style]}>",
      "      <Text>{Strings.appName}</Text>",
      "    </View>",
      "  );",
      "};",
      "",
      "export default $0;",
      ""
    ],
